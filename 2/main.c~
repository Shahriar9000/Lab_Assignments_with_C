#include <stdio.h> // for printf()
#include "funcs.h" 

int main( void )
{
  int a = 11;
  int b = 12;
  printf( "The max of our numbers is %d\n", max( a, b ));
  printf( "The min of our numbers is %d\n", min( a, b ));
  return 0;
}


int main( void )
{
  int a[3] = {10, 15, 20};
  int b[3] = {10, 20, 15};
 
  if( identical( a, b, 3 ) == 1 )
  {
     printf( "a and b are identical\n" );
  } else {
    printf( "a and b are NOT identical\n" );
  }
  
  return 0;
}
